<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
	</configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<log4net>
		<appender name="TestAppender" type="log4net.Appender.RollingFileAppender">
			<file type="log4net.Util.PatternString" value="%property{LogFileName}"/>
			<encoding value="utf-8" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datePattern value="yyyyMMdd" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date > [%logger]{%method} > %level:: %message%n" />
			</layout>
		</appender>
		<root>
			<level value="All" />
			<appender-ref ref="TestAppender" />
		</root>
	</log4net>
    <appSettings>
		<add key="Interval" value="15000" />
		<add key="StartDateTime" value="18/04/2025 01:20:00" />
		<add key="EndDateTime" value="18/04/2025 01:30:00" />
		<add key="Width" value="" />
		<add key="Height" value="" />
		<add key="Percent" value="0,50" />
		<add key="Folder" value="C:\$SpreadWork\" />
		<add key="RemoteConfigPath" value="\\w7pro\fotos\config\"/>
		<add key="RemoteFilesPath" value="\\w7pro\fotos\files\"/>
		<add key="Event" value="Any"/>
    </appSettings>
</configuration>


